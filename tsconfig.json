{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/sidebar.ts",
    "pages/checkInScreen.jks",
    "components/userFound.js",
    "components/userNotFound.js",
    "pages/index.js",
    "components/sidebar.js",
    "pages/_app.js",
    "context/global-context.js"
  ],
  "exclude": ["node_modules"]
}
